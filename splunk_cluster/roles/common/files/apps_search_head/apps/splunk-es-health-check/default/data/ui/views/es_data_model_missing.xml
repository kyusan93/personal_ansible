<form theme="dark" version="1.1">
  <label>CIM Data Models - Potentially excluded Sourcetypes</label>
  <fieldset submitButton="true">
    <input type="time" token="time" searchWhenChanged="true">
      <label></label>
      <default>
        <earliest>-24h@h</earliest>
        <latest>now</latest>
      </default>
    </input>
  </fieldset>
  <row>
    <panel depends="$show_alerts_panel$">
      <title>Alerts</title>
      <html>No issues identified</html>
    </panel>
    <panel rejects="$show_alerts_panel$">
      <title>Alerts</title>
      <table>
        <search>
          <query>(index=* NOT (`cim_Alerts_indexes`) tag=alert) | eventstats values(tag) AS tags by _raw | nomv tags | stats count by sourcetype index tags</query>
          <earliest>$time.earliest$</earliest>
          <latest>$time.latest$</latest>
          <progress>
            <condition match="'job.resultCount'== 0">
              <set token="show_alerts_panel">1</set>
            </condition>
            <condition>
              <unset token="show_alerts_panel"></unset>
            </condition>
          </progress>
        </search>
        <option name="count">10</option>
        <option name="dataOverlayMode">none</option>
        <option name="drilldown">none</option>
        <option name="percentagesRow">false</option>
        <option name="rowNumbers">false</option>
        <option name="totalsRow">false</option>
        <option name="wrap">true</option>
      </table>
    </panel>
  </row>
